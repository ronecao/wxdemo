<view class='container vertical centered'>
  <view wx:if = '{{ header.show }}' >
    <text class='{{ header.class }}'> {{ header.content ? header.content : "emtpy" }} </text>
  </view>
  <view wx:else >
    <text class='{{ header.class }}'> branch </text>
  </view>
  <view wx:for='{{ fruits }}' class="item" wx:for-item="fruit" >
    {{ index }}: {{ fruit }}
  </view>
</view>
<view class="image">
  <image src="../../assets/images/landscape.png"></image>
</view>
